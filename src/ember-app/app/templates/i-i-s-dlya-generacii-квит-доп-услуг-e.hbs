<h3 class="ui header">{{t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-dlya-generacii-квит-доп-услуг-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерКвит
      class=(if (v-get validationModel "номерКвит" "isInvalid") "error")
      label=(t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.номерКвит-caption")
      data-test-i-i-s-dlya-generacii-квит-доп-услуг-e-номерКвит=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерКвит" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.итогСтоим
      class=(if (v-get validationModel "итогСтоим" "isInvalid") "error")
      label=(t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.итогСтоим-caption")
      data-test-i-i-s-dlya-generacii-квит-доп-услуг-e-итогСтоим=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "итогСтоим" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.клиенты-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.клиенты
      displayAttributeName="фИО"
      autocomplete=true
      relationName="клиенты"
      projection="КлиентыL"
      title=(t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.клиенты-caption")
      readonly=readonly
      componentName="клиентыLookup"
      class=(if (v-get validationModel "клиенты" "isInvalid") "error")
      data-test-i-i-s-dlya-generacii-квит-доп-услуг-e-клиенты=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "клиенты" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.тЧПереченьУсл-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.тЧПереченьУсл
      displayAttributeName="количество"
      autocomplete=true
      relationName="тЧПереченьУсл"
      projection="ТЧПереченьУслL"
      title=(t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.тЧПереченьУсл-caption")
      readonly=readonly
      componentName="тЧПереченьУслLookup"
      class=(if (v-get validationModel "тЧПереченьУсл" "isInvalid") "error")
      data-test-i-i-s-dlya-generacii-квит-доп-услуг-e-тЧПереченьУсл=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тЧПереченьУсл" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.организация-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.организация
      displayAttributeName="наименование"
      autocomplete=true
      relationName="организация"
      projection="ОрганизацияL"
      title=(t "forms.i-i-s-dlya-generacii-квит-доп-услуг-e.организация-caption")
      readonly=readonly
      componentName="организацияLookup"
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      data-test-i-i-s-dlya-generacii-квит-доп-услуг-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
</form>
